{
  "name": "@project/fe-v3-naive",
  "version": "1.8.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "serve": "serve dist",
    "build:prod": "vite build && esno ./build/script/postBuild.ts",
    "build:no-cache": "yarn clean:cache && npm run build",
    "report": "cross-env REPORT=true npm run build",
    "preview": "npm run build && vite preview",
    "preview:dist": "vite preview",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "build typecheck": "vuedx-typecheck . && vite build",
    "lint:eslint": "eslint \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "test prod gzip": "http-server dist --cors --gzip -c-1"
  },
  "dependencies": {
    "@monorepo/config": "workspace:*",
    "@monorepo/http": "workspace:*",
    "@monorepo/utils": "workspace:*",
    "@vicons/antd": "^0.12.0",
    "@vicons/ionicons5": "^0.12.0",
    "@vueup/vue-quill": "^1.0.0",
    "@vueuse/core": "^9.6.0",
    "axios": "^1.2.1",
    "blueimp-md5": "^2.19.0",
    "date-fns": "^2.29.3",
    "echarts": "^5.4.1",
    "element-resize-detector": "^1.2.4",
    "mitt": "^3.0.0",
    "naive-ui": "^2.34.2",
    "pinia": "^2.0.28",
    "vfonts": "^0.0.3",
    "vue": "^3.2.45",
    "vue-demi": "^0.13.11",
    "vue-router": "^4.1.6",
    "vue-types": "^5.0.1",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.15",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/compiler-sfc": "^3.2.45",
    "@vue/eslint-config-typescript": "^11.0.2",
    "core-js": "^3.26.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "1.12.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.8.0",
    "esno": "^0.16.3",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "unplugin-vue-components": "^0.22.12",
    "vite": "3.2.5",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-windicss": "^1.8.10",
    "vue-eslint-parser": "^9.1.0",
    "windicss": "^3.5.6"
  },
  "engines": {
    "node": "^12 || >=14"
  }
}
